<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sagarFitnessApp.test1"
    android:versionCode="4"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon3"
        android:label="@string/label"
        android:theme="@style/Sherlock.__Theme" >
        <activity
            android:name="com.sagarFitnessApp.test1.MainActivity"
            android:label="@string/app_name" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sagarFitnessApp.test1.NewLog"
            android:label="@string/new_log" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.sagarFitnessApp.test1.NEWLOG" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sagarFitnessApp.test1.Logs"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/app_name" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="com.sagarFitnessApp.test1.SAVEDLOG" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sagarFitnessApp.test1.History"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_history" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.sagarFitnessApp.test1.HISTORY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sagarFitnessApp.test1.MyTabListener"
            android:label="@string/title_activity_my_tab_listener" >
        </activity>
        <activity
            android:name="com.sagarFitnessApp.test1.About"
            android:label="@string/title_activity_about" android:screenOrientation="portrait">
                        <intent-filter>
            <action android:name="com.sagarFitnessApp.test1.ABOUT" />

                <category android:name="android.intent.category.DEFAULT" />
                            </intent-filter>
        </activity>
    </application>

</manifest>